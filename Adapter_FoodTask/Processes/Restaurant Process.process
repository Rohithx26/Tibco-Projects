<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/jms" xmlns:pfx="http://www.tibco.com/schemas/FoodOrdering/Schemas/Schema.xsd2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/FoodOrdering/Schemas/Schema.xsd2" schemaLocation="/Schemas/Food_Order.xsd"/>
    <pd:name>Processes/Restaurant Process.process</pd:name>
    <pd:startName>JMS Queue Receiver</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="JMS Queue Receiver">
        <pd:type>com.tibco.plugin.jms.JMSQueueEventSource</pd:type>
        <pd:resourceType>ae.activities.JMSQueueEventSource</pd:resourceType>
        <pd:x>134</pd:x>
        <pd:y>114</pd:y>
        <config>
            <PermittedMessageType>XML Text</PermittedMessageType>
            <SessionAttributes>
                <transacted>false</transacted>
                <acknowledgeMode>1</acknowledgeMode>
                <maxSessions>1</maxSessions>
                <destination>order.queue</destination>
            </SessionAttributes>
            <ConfigurableHeaders>
                <JMSDeliveryMode>PERSISTENT</JMSDeliveryMode>
                <JMSExpiration>0</JMSExpiration>
                <JMSPriority>4</JMSPriority>
            </ConfigurableHeaders>
            <ConnectionReference>/Connections/JMS/JMS Connection.sharedjmscon</ConnectionReference>
            <OutDataxsdString ref="pfx:FoodOrder"/>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>855</pd:endX>
    <pd:endY>108</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1731996442795</pd:targetNamespace>
    <pd:activity name="Check Availability">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>341</pd:x>
        <pd:y>116</pd:y>
        <config>
            <processName>/Processes/Check Availability.process</processName>
        </config>
        <pd:inputBindings>
            <Menu>
                <Food_Item>
                    <xsl:value-of select="$JMS-Queue-Receiver/ns:ActivityOutput/Body/pfx:FoodOrder/pfx:Response/pfx:food_item"/>
                </Food_Item>
                <QuantityNeed>
                    <xsl:value-of select="$JMS-Queue-Receiver/ns:ActivityOutput/Body/pfx:FoodOrder/pfx:Response/pfx:quantity"/>
                </QuantityNeed>
            </Menu>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Update Pending Order - Accept">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>530</pd:x>
        <pd:y>43</pd:y>
        <config>
            <processName>/Processes/Update Pending Orders-Accept.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <order_id>
                    <xsl:value-of select="$JMS-Queue-Receiver/ns:ActivityOutput/Body/pfx:FoodOrder/pfx:Response/pfx:order_id"/>
                </order_id>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Update Pending Order - Reject">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>532</pd:x>
        <pd:y>186</pd:y>
        <config>
            <processName>/Processes/Update Pending Order - Rejected.process</processName>
        </config>
        <pd:inputBindings>
            <root>
                <order_id>
                    <xsl:value-of select="$JMS-Queue-Receiver/ns:ActivityOutput/Body/pfx:FoodOrder/pfx:Response/pfx:order_id"/>
                </order_id>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Order Response">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>711</pd:x>
        <pd:y>106</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/Connections/JDBC/JDBC Connection.sharedjdbc</jdbcSharedConfig>
            <statement>insert into OrderResponse values(?,?,?,?)</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>order_id</parameterName>
                    <dataType>INTEGER</dataType>
                </parameter>
                <parameter>
                    <parameterName>response_message</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>response_status</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>Timestamp</parameterName>
                    <dataType>TIMESTAMP</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <order_id>
                    <xsl:choose>
                        <xsl:when test="exists($JMS-Queue-Receiver/ns:ActivityOutput/Body/pfx:FoodOrder/pfx:Response/pfx:order_id)">
                            <xsl:value-of select="$JMS-Queue-Receiver/ns:ActivityOutput/Body/pfx:FoodOrder/pfx:Response/pfx:order_id"/>
                        </xsl:when>
                        <xsl:otherwise>
                            <xsl:attribute name="xsi:nil">true</xsl:attribute>
                        </xsl:otherwise>
                    </xsl:choose>
                </order_id>
                <xsl:choose>
                    <xsl:when test="$Update-Pending-Order---Accept/CompletionStatus/OrderStatus">
                        <response_message>
                            <xsl:value-of select="&quot;Order Accepted...Thank You&quot;"/>
                        </response_message>
                    </xsl:when>
                    <xsl:otherwise>
                        <response_message>
                            <xsl:value-of select="&quot;Order Rejected due to insufficient quantity&quot;"/>
                        </response_message>
                    </xsl:otherwise>
                </xsl:choose>
                <xsl:choose>
                    <xsl:when test="$Update-Pending-Order---Accept/CompletionStatus/OrderStatus">
                        <response_status>
                            <xsl:value-of select="&quot;ACCEPTED&quot;"/>
                        </response_status>
                    </xsl:when>
                    <xsl:otherwise>
                        <response_status>
                            <xsl:value-of select="&quot;REJECTED&quot;"/>
                        </response_status>
                    </xsl:otherwise>
                </xsl:choose>
                <Timestamp>
                    <xsl:value-of select="current-dateTime()"/>
                </Timestamp>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Check Availability</pd:from>
        <pd:to>Update Pending Order - Accept</pd:to>
        <pd:xpathDescription>Accepted</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>otherwise</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Check Availability</pd:from>
        <pd:to>Update Pending Order - Reject</pd:to>
        <pd:xpathDescription>Rejected</pd:xpathDescription>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>xpath</pd:conditionType>
        <pd:xpath>$Check-Availability/root/Availability="0"</pd:xpath>
    </pd:transition>
    <pd:transition>
        <pd:from>JMS Queue Receiver</pd:from>
        <pd:to>Check Availability</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Update Pending Order - Accept</pd:from>
        <pd:to>Order Response</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Update Pending Order - Reject</pd:from>
        <pd:to>Order Response</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Order Response</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>